ALGORITHM InsertionSort;
VAR
    set1 : ARRAY_OF INTEGERs[12, 11, 13, 5, 6]
    i, j : INTEGER;
BEGIN
    FOR i FROM 0 TO  array.length-1 STEP 1 DO
    FOR J FROM 0 TO array.length-i-1 STEP 1 DO
    IF (arr[j] > arr[j+1]) THEN
    swap(arr[j], arr[j+1])
    END_IF
    END_FOR
    END_FOR
END
